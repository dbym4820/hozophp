{
    "name": "dbym4820/hozophp",
    "type": "library",
    "description": "HozoPHP is a ontology data management library for treat hozo-formed ontology",
    "homepage": "https://github.com/dbym4820/hozophp",
    "keywords": ["hozo", "xml", "ontology"],
    "authors": [{
	"name": "dbym4820",
	"email": "aburatanitomoki@gmail.com"
    }],
    "require": {
	"php": ">=8.0.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
	"psr-4": {
	    "HozoPHP\\": [
		"src"
	    ]
	}
    },
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "HozoPHP\\Tests\\": "tests"
        }
